        static int Maior(int andar1, int andar2, int andar3, int andar4, int andar5)
        {
            int maiorNumero;

            if (andar2 > andar1 && andar3 > andar2)
            {
                maiorNumero = andar3;
            }
            else if (andar3 > andar2)
            {
                maiorNumero = andar3;
            }
            else
            {
                maiorNumero = andar3;
            }


            return maiorNumero;
        }

