        static void NumeroMaior()
        {


            Console.WriteLine("Digite andar desejado: ");

            int andar1, andar2, andar3, andar4, andar5;

            andar1 = int.Parse(Console.ReadLine());
            andar2 = int.Parse(Console.ReadLine());
            andar3 = int.Parse(Console.ReadLine());
            andar4 = int.Parse(Console.ReadLine());
            andar5 = int.Parse(Console.ReadLine());



            int resultado = Maior(andar1, andar2, andar3, andar4, andar5);

            Console.WriteLine("Maior = " + resultado);

